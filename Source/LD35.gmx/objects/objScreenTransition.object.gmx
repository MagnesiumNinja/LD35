          <argument>
            <kind>1</kind>
            <string>// Surface drawing
if (!surface_exists(surface))
    surface = surface_create(view_wport[0], view_hport[0]);

surface_set_target(surface);

draw_clear(c_black);

draw_set_blend_mode(bm_subtract);

draw_set_color(c_white);
draw_circle(view_wport[0] / 2, view_hport[0] / 2, radius, false);

draw_set_blend_mode(bm_normal);

surface_reset_target();


// State changing
switch(state)
{
    case 0:
    {
        if (radius &gt; 0)
            radius = approach(radius, 0, CIRCLE_SPEED);
        else
        {
            room_goto(rm);
            state++;
        }
        
        break;
    }
    
    case 1:
    {
        if (wait &gt; 0)
            wait--;
        else
            state++;
        break;
    }
    
    case 2:
    {
        if (radius &lt; view_wport[0] / 2)
            radius = approach(radius, view_wport[0] / 2, CIRCLE_SPEED);
        else
            instance_destroy();
        break;
    }
}
</string>
